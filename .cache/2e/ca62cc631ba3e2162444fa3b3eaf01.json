 {\n    color: white;\n    @include mixins.fluid-font(2vw, 15px, 15px, 20px);\n    font-weight: 300;\n  }","scss/_animations.scss":"@keyframes rotate-in {\n  0% {\n    transform: rotateX(180deg);\n    opacity: 0;\n  }\n  35% {\n    transform: rotateX(120deg);\n    opacity: 0;\n  }\n  65% {\n    opacity: 0;\n  }\n  100% {\n    transform: rotateX(360deg);\n    opacity: 1;\n  }\n}\n\n@keyframes rotate-out {\n  0% {\n    transform: rotateX(0deg);\n    opacity: 1;\n  }\n  35% {\n    transform: rotateX(-40deg);\n    opacity: 1;\n  }\n  65% {\n    opacity: 0;\n  }\n  100% {\n    transform: rotateX(180deg);\n    opacity: 0;\n  }\n}\n","scss/_basic.scss":"@use 'variables';\n@use 'mixins';\n\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n  font-family: \"Roboto\", sans-serif;\n  color: #181818;\n}\nhtml {\n  box-sizing: border-box;\n  scroll-behavior: smooth;\n}\n*,\n*:before,\n*:after {\n  box-sizing: inherit;\n}\n\n.container {\n  max-width: 1200px;\n  margin: auto;\n  // display: flex;\n  // flex-direction: column;\n}\n\n.card {\n  background-color: variables.$accent;\n  border-radius: 10px;\n  padding: 10px;\n  margin: 10px;\n  box-shadow: 0 2px 1px -1px rgba(0, 0, 0, 0.2), 0 1px 1px 0 rgba(0, 0, 0, 0.14),\n    0 1px 3px 0 rgba(0, 0, 0, 0.12);\n  transition: ease-in-out 200ms;\n\n  &:hover {\n    box-shadow: 0 2px 4px -1px rgba(0, 0, 0, 0.2),\n      0 4px 5px 0 rgba(0, 0, 0, 0.14), 0 1px 10px 0 rgba(0, 0, 0, 0.12);\n  }\n}\n\n.is-visible {\n  opacity: 1;\n}\n\n.is-hidden {\n  opacity: 0;\n}\n\n.drawer {\n  z-index: 10;\n  position: fixed;\n  background-color: grey;\n  height: 100vh;\n  width: 100vw;\n  top: 0;\n  right: 0;\n  box-shadow: 0 3px 3px -2px rgba(0, 0, 0, 0.2), 0 3px 4px 0 rgba(0, 0, 0, 0.14),\n    0 1px 8px 0 rgba(0, 0, 0, 0.12);\n  transition: ease-in-out 100ms;\n  pointer-events: auto;\n  h1 {\n    padding: 10px;\n  }\n\n  // .image {\n  //   background-image: radial-gradient(transparent, black),\n  //     url(../img/Room2.37.jpg);\n  //   background-repeat: no-repeat;\n  //   background-size: cover;\n  //   height: 50vh;\n  //   max-width: 100%;\n\n  //   transition: ease-in-out 500ms;\n  // }\n  // .image:hover {\n  //   background-image: radial-gradient(transparent, transparent),\n  //     url(../img/Room2.37.jpg);\n  //   max-width: 100%;\n\n  //   transition: ease-in-out 500ms;\n  // }\n}\n\n.drawer--closed {\n  opacity: 0;\n  transform: scale(0.99);\n  transition: ease-in-out 100ms;\n  pointer-events: none;\n}\n\nbutton {\n  z-index: 20;\n  position: relative;\n}\n","scss/_variables.scss":"$bg-color: #edf1df;\n$accent: #ffffff;\n\n$yellow: #f9cb85;\n$blue: #337fba;\n$purple: #8c73a9;\n$orange: #ef7457;\n$green: #72b498;\n$pink: #f0a6b9;\n","scss/_home.scss":"@use 'variables';\n@use 'mixins';\n\n\nbody {\n  background-color: variables.$bg-color;\n}\n.grid {\n  display: flex;\n  flex-wrap: wrap;\n\n  .cell {\n    position: relative;\n    // background-color: #f1f1f1;\n    padding: 0;\n    flex: 1 30%;\n    min-height: 50px;\n\n    .label {\n      padding: 2%;\n\n      h3 {\n        margin-bottom: 5px;\n      }\n    }\n\n    img {\n      max-width: 100%;\n      max-height: 100%;\n      object-fit: cover;\n      border-radius: 10px;\n    }\n  }\n}\n\n.id {\n  display: flex;\n  // justify-content: space-around;\n  align-items: center;\n\n  img {\n    max-height: 250px;\n    margin: 20px;\n  }\n}\n\n.intro{\n\n&.rotate .wordWrapper {\n  display: inline-block;\n  perspective: 300px;\n  width: 8rem;\n  margin: 0 auto;\n}\n\n&.rotate b {\n  opacity: 0;\n  transform-origin: 50% 100%;\n  transform: rotateX(180deg);\n  display: inline-block;\n  position: absolute;\n  white-space: nowrap;\n  // left: 0;\n  top: 0;\n}\n\n&.rotate b.is-visible {\n  position: relative;\n  opacity: 1;\n  transform: rotateX(0deg);\n  animation: rotate-in 1.2s;\n}\n\n&.rotate b.is-hidden {\n  transform: rotateX(180deg);\n  animation: rotate-out 1.2s;\n  opacity: 0;\n}\n}\n\n\n.color1{\n  @include mixins.cardColor(variables.$yellow);\n}\n.color2{\n  @include mixins.cardColor(variables.$orange);\n}\n.color3{\n  @include mixins.cardColor(variables.$green);\n}\n.color4{\n  @include mixins.cardColor(variables.$purple);\n}\n.color5{\n  @include mixins.cardColor(variables.$blue);\n}\n.color6{\n  @include mixins.cardColor(variables.$pink);\n}\n","scss/_mixins.scss":"\n@use \"sass:color\";\n\n@mixin fluid-font($responsive, $min, $max: false, $fallback: false) {\n  $responsive-unitless: $responsive / ($responsive - $responsive + 1);\n  $dimension: if(unit($responsive) == \"vh\", \"height\", \"width\");\n  $min-breakpoint: $min / $responsive-unitless * 100;\n  @media (max-#{$dimension}: #{$min-breakpoint}) {\n    font-size: $min;\n  }\n  @if $max {\n    $max-breakpoint: $max / $responsive-unitless * 100;\n    @media (min-#{$dimension}: #{$max-breakpoint}) {\n      font-size: $max;\n    }\n  }\n  @if $fallback {\n    font-size: $fallback;\n  }\n  font-size: $responsive;\n}\n\n@mixin cardColor($bgcolor){\nbackground-color: $bgcolor;\n\nh2 {\n  color: color.change($bgcolor, $lightness: 20%);\n}\nh4{\n  color: color.change($bgcolor, $lightness: 10%);\n\n}\n}\n","scss/_drawer.scss":".slide {\n  background-color: white;\n  position: fixed;\n  top: 5vw;\n  right: -15vw;\n  // height: 100vh;\n  width: 15vw;\n  transition: all 500ms ease-in-out 250ms;\n  transform: translateX(-15vw);\n  // &:hover {\n  //   // transform: translateX(-15vw);\n  // }\n}\n.slide .link {\n  position: relative;\n  // height: 25%;\n}\n\n.slide h3 {\n  z-index: 2;\n  position: absolute;\n  top: 50%;\n  transform: translate(0, -50%);\n  width: 100%;\n  text-align: center;\n  color: white;\n  font-weight: 600;\n  pointer-events: none;\n}\n\n.triangle {\n  fill: #337fba;\n  transition: fill 100ms;\n  :hover {\n    fill: #18517c;\n  }\n}\n\n.nav {\n  display: flex;\n  flex-direction: column;\n  padding: 10px;\n}\n","scss/_query.scss":"@media (max-width: 30rem) {\n    .grid {\n      display: flex;\n      flex-wrap: wrap;\n  \n      .cell {\n  