items: center;\r\n\r\n  img {\r\n    max-height: 250px;\r\n    margin: 20px;\r\n  }\r\n}\r\nh1 {\r\n  @include mixins.fluid-font(4vw, 25px, 35px, 30px);\r\n}\r\n\r\nh3 {\r\n  @include mixins.fluid-font(2vw, 15px, 20px, 20px);\r\n  font-weight: 300;\r\n}\r\n\r\nh4 {\r\n  @include mixins.fluid-font(2vw, 15px, 15px, 20px);\r\n}\r\n\r\n.is-visible {\r\n  opacity: 1;\r\n}\r\n\r\n.is-hidden {\r\n  opacity: 0;\r\n}\r\n\r\n.intro.rotate .wordWrapper {\r\n  display: inline-block;\r\n  perspective: 300px;\r\n  width: 8rem;\r\n  margin: 0 auto;\r\n}\r\n\r\n.intro.rotate b {\r\n  opacity: 0;\r\n  transform-origin: 50% 100%;\r\n  transform: rotateX(180deg);\r\n  display: inline-block;\r\n  position: absolute;\r\n  white-space: nowrap;\r\n  // left: 0;\r\n  top: 0;\r\n}\r\n\r\n.intro.rotate b.is-visible {\r\n  position: relative;\r\n  opacity: 1;\r\n  transform: rotateX(0deg);\r\n  animation: rotate-in 1.2s;\r\n}\r\n\r\n.intro.rotate b.is-hidden {\r\n  transform: rotateX(180deg);\r\n  animation: rotate-out 1.2s;\r\n  opacity: 0;\r\n}\r\n\r\n.drawer {\r\n  z-index: 10;\r\n  position: fixed;\r\n  background-color: white;\r\n  height: 100vh;\r\n  width: 100vw;\r\n  top: 0;\r\n  right: 0;\r\n  box-shadow: 0 3px 3px -2px rgba(0, 0, 0, 0.2), 0 3px 4px 0 rgba(0, 0, 0, 0.14),\r\n    0 1px 8px 0 rgba(0, 0, 0, 0.12);\r\n  transition: ease-in-out 100ms;\r\n  pointer-events: auto;\r\n  h1 {\r\n    padding: 10px;\r\n  }\r\n\r\n  // .image {\r\n  //   background-image: radial-gradient(transparent, black),\r\n  //     url(../img/Room2.37.jpg);\r\n  //   background-repeat: no-repeat;\r\n  //   background-size: cover;\r\n  //   height: 50vh;\r\n  //   max-width: 100%;\r\n\r\n  //   transition: ease-in-out 500ms;\r\n  // }\r\n  // .image:hover {\r\n  //   background-image: radial-gradient(transparent, transparent),\r\n  //     url(../img/Room2.37.jpg);\r\n  //   max-width: 100%;\r\n\r\n  //   transition: ease-in-out 500ms;\r\n  // }\r\n}\r\n\r\n.drawer--closed {\r\n  opacity: 0;\r\n  transform: scale(0.99);\r\n  transition: ease-in-out 100ms;\r\n  pointer-events: none;\r\n}\r\n\r\nbutton {\r\n  z-index: 20;\r\n  position: relative;\r\n}\r\n\r\n@media (max-width: 30rem) {\r\n  .grid {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n\r\n    .cell {\r\n      flex: 1 1 100%;\r\n      min-height: 50px;\r\n    }\r\n  }\r\n\r\n  .id {\r\n    display: flex;\r\n    flex-direction: column;\r\n\r\n    img {\r\n      max-height: 250px;\r\n      margin: 10px;\r\n    }\r\n    h1 {\r\n      text-align: center;\r\n      // font-size: 6vw;\r\n    }\r\n    h3 {\r\n      text-align: center;\r\n      // font-size: 4vw;\r\n    }\r\n  }\r\n  .intro.rotate {\r\n    .wordWrapper {\r\n      width: 12rem;\r\n    }\r\n  }\r\n}\r\n","scss\\_animations.scss":"@keyframes rotate-in {\r\n  0% {\r\n    transform: rotateX(180deg);\r\n    opacity: 0;\r\n  }\r\n  35% {\r\n    transform: rotateX(120deg);\r\n    opacity: 0;\r\n  }\r\n  65% {\r\n    opacity: 0;\r\n  }\r\n  100% {\r\n    transform: rotateX(360deg);\r\n    opacity: 1;\r\n  }\r\n}\r\n\r\n@keyframes rotate-out {\r\n  0% {\r\n    transform: rotateX(0deg);\r\n    opacity: 1;\r\n  }\r\n  35% {\r\n    transform: rotateX(-40deg);\r\n    opacity: 1;\r\n  }\r\n  65% {\r\n    opacity: 0;\r\n  }\r\n  100% {\r\n    transform: rotateX(180deg);\r\n    opacity: 0;\r\n  }\r\n}\r\n","scss\\_variables.scss":"$bg-color: #edf1df;\r\n$accent: #ffffff;\r\n","scss\\_mixins.scss":"@mixin fluid-font($responsive, $min, $max: false, $fallback: false) {\r\n  $responsive-unitless: $responsive / ($responsive - $responsive + 1);\r\n  $dimension: if(unit($responsive) == \"vh\", \"height\", \"width\");\r\n  $min-breakpoint: $min / $responsive-unitless * 100;\r\n  @media (max-#{$dimension}: #{$min-breakpoint}) {\r\n    font-size: $min;\r\n  }\r\n  @if $max {\r\n    $max-breakpoint: $max / $responsive-unitless * 100;\r\n    @media (min-#{$dimension}: #{$max-breakpoint}) {\r\n      font-size: $max;\r\n    }\r\n  }\r\n  @if $fallback {\r\n    font-size: $fallback;\r\n  }\r\n  font-size: $responsive;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"15ce15cb8a0b3eb8482b0baa9f5b264e","cacheData":{"env":{}}}